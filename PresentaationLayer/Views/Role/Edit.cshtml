@model RoleViewModel
@{
    ViewData["Title"] = "Edit";
}
 @* @TempData["Message"]; *@

<form method="post" class="form" asp-action="Edit" enctype="multipart/form-data">
    @* <div asp-validation-summary="All"></div> *@
    <partial name="PartialViews/_CreateEditPartial"></partial>
    <div class="mt-3">
        <a class="btn btn-info" asp-action="AddOrRemoveUserRoles" asp-route-roleId="@Model.Id">Add Or Remove Users</a>
    </div>
    <div class="mt-3">
        <button class="btn btn-success">Update</button>
        <a class="btn btn-danger" asp-action="Index">Cancel</a>
    </div>
</form>
@section Validation {
    <script src="~/lib/jquery-validation/dist/additional-methods.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
}